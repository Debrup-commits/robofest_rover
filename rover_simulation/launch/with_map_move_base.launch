<launch>
    <node pkg="move_base" type="move_base" respawn="false" name="move_base" output="screen">
        <rosparam file="$(find robofest_rover)/params/costmap_common_params.yaml" command="load" ns="global_costmap" />
        <rosparam file="$(find robofest_rover)/params/costmap_common_params.yaml" command="load" ns="local_costmap" /> 

        <rosparam file="$(find robofest_rover)/params/local_costmap_params.yaml" command="load" /> 
        <rosparam file="$(find robofest_rover)/params/global_costmap_params.yaml" command="load" /> 
        
        <rosparam file="$(find robofest_rover)/params/base_local_planner_params.yaml" command="load"  /> 
        <rosparam file="$(find robofest_rover)/params/move_base_params.yaml" command="load" /> 

        <param name="base_global_planner" type="string" value="navfn/NavfnROS" />
        <param name="base_local_planner" value="base_local_planner/TrajectoryPlannerROS"/>

        <remap from="jackal_velocity_controller/odom" to="odometry/filtered"/>

    </node>
</launch>